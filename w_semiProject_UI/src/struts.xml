<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
<constant name="struts.i18n.encoding" value="EUC-KR"/>
<package name="default" namespace="/" extends="struts-default">
<!-- documentRoot의 경로를 index로 요청을 하라는 뜻 -->
<default-action-ref name="index" />
<action name="index">
	<result>/index.jsp</result>
</action>
</package>
<include file="z_calendar/struts-calendar.xml"/>
<include file="z_chart/struts-chart.xml"/>
<include file="z_img/struts-img.xml"/>
<include file="z_join/struts-join.xml"/>
<include file="z_board/struts-board.xml"/>
</struts>

